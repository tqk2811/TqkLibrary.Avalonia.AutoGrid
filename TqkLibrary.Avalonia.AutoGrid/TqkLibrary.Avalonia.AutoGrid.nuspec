<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
	<!--https://docs.microsoft.com/en-us/nuget/reference/nuspec-->
	<metadata>
		<id>$id$</id>
		<version>1.0.1</version>
		<description>Magical replacement for the default WPF Grid.</description>
		<authors>tqk2811</authors>
		<readme>docs\README.md</readme>
		<projectUrl>https://github.com/tqk2811/TqkLibrary.Avalonia.AutoGrid</projectUrl>
		<repository type="git" url="https://github.com/tqk2811/TqkLibrary.Avalonia.AutoGrid.git"/>
		<license type="expression">MIT</license>
		<dependencies>
			<group targetFramework="netstandard2.0">
				<dependency id="Avalonia" version="11.3.8" />
			</group>
			<group targetFramework="net6.0">
				<dependency id="Avalonia" version="11.3.8" />
			</group>
			<group targetFramework="net8.0">
				<dependency id="Avalonia" version="11.3.8" />
			</group>
		</dependencies>
	</metadata>
	<files>
		<file src="bin\Release\**\$id$.dll" target="lib\" />
		<file src="bin\Release\**\$id$.pdb" target="lib\" />
		<file src="bin\Release\**\$id$.xml" target="lib\" />
		<file src="**\*.cs" target="src\" exclude="obj\**\*.cs;bin\**\*.cs"/>
		<file src="..\README.md" target="docs\" />
	</files>
</package>